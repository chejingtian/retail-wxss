/* pages/goods/goods-detail/goodsDetail.wxss */

page {
  height: 100%;
  overflow-y: hidden;
}

text {
}

.nav-bar {
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  top: 0;
  z-index: 99;
  display: flex;
  border-top: 1px solid #eee;
  box-shadow: 1rpx 1rpx 0 rgba(0, 0, 0, 0.08);
}

.nav-bar text {
  display: inline-block;
  width: 25%;
  font-size: 30rpx;
  font-weight: bold;
  color: #666;
  text-align: center;
  line-height: 36rpx;
  position: relative;
}
.nav-active{
  color:#deaf80;
}

.nav-bar text::after {
  content: '';
  width: 2rpx;
  height: 40rpx;
  position: absolute;
  right: 0rpx;
  top: 30rpx;
  background: #999;
}

.nav-bar text:last-child::after {
  background: #fff;
}

.nav-bar .active {
  color: #dfaf7d;
}

.title {
  padding: 30rpx 0 30rpx 20rpx;
}

.title .subTitle {
  font-size: 12px;
  color: #999;
}

.line {
  width: 100%;
  height: 10rpx;
  background: #f1f1f1;
}

.shopInfo {
  min-height: 156rpx;
}

.shopInfo .info {
  padding: 22rpx 15rpx;
  position: relative;
}

.shopInfo .info image {
  float: left;
  margin-right: 30rpx;
}

.shopInfo .info .shop-title {
  margin: 20rpx 0 20rpx 20rpx;
  line-height: 20rpx;
  height: 20rpx;
}

.shopInfo .info .shop-position {
  font-size: 24rpx;
  color: rgb(102, 102, 102);
}

.shopInfo .info .btn-toShop {
  width: 128rpx;
  height: 48rpx;
  border: 2rpx solid rgb(223, 175, 125);
  color: rgb(223, 175, 125);
  font-size: 24rpx;
  text-align: center;
  line-height: 48rpx;
  position: absolute;
  right: 30rpx;
  top: 50rpx;
  border-radius: 4rpx;
}

.productShortInfo .market-price {
  padding-left: 20rpx;
  color: #999;
  font-size: 12px;
  font-family: PingFang SC;
  position: relative;
}

.productShortInfo .market-price ::after {
  position: absolute;
  content: '';
  width: 110rpx;
  height: 2rpx;
  background: #999;
  left: 15rpx;
  top: 50%;
}

.productShortInfo .dispatch-range {
  padding: 10rpx 0 40rpx 20rpx;
  font-size: 12px;
  color: rgb(102, 102, 102);
}

.productShortInfo .market-promotion {
  padding-left: 20rpx;
  color: #666;
  font-weight: 700;
  position: relative;
}

.productShortInfo .market-promotion .price {
  font-family: PingFang SC;
  color: #ff454a;
  font-size: 20px;
}

.modules .detail {
  font-size: 12px;
  color: #666;
  padding-left: 20rpx;
  height: 35px;
}

.modules .detail .detail-title {
  width: 100px;
  float: left;
}

.count-down-container {
  height: 100rpx;
  width: 100%;
  background: #dfaf7d;
  padding: 0 20rpx;
}

.count-down-container .count-title {
  float: left;
}

.count-down-container .count-down-box {
  float: right;
  margin-right: 100rpx;
  font-size: 24rpx;
  color: #fff;
  margin: 10rpx 100rpx 10rpx 0;
}

.market-promotion .repertory {
  font-size: 24rpx;
  font-weight: normal;
  float: right;
  margin-right: 40rpx;
  margin-top: 20rpx;
}

.productShortInfo .rule-wrap {
  width: 100%;
  height: 86rpx;
  padding: 0 40rpx;
  overflow: hidden;
  color: #333;
  font-size: 24rpx;
  background: #fafafa;
  padding-bottom: 10rpx;
  margin-bottom: 40rpx;
  position: relative;
}

.productShortInfo .rule-wrap .arrow {
  position: absolute;
  right: 120rpx;
  top: 40rpx;
  width: 7px;
  height: 7px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  transform: rotate(45deg);
}

.productShortInfo .group-booking .arrow {
  position: absolute;
  right: 120rpx;
  top: 40rpx;
  width: 7px;
  height: 7px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  transform: rotate(45deg);
}

/*弹出层样式  */

.buydes-dialog-container {
  width: 100%;
  height: 100%;
  justify-content: space-between;
  background-color: rgba(15, 15, 26, 0.7);
  position: fixed;
  z-index: 999;
  top: 0;
  display: flex;
  flex-direction: column;
}

.buydes-dialog-container-top {
  flex-grow: 1;
}

.buydes-dialog-container-bottom {
  flex-grow: 0;
  bottom: 0;
  background: #fff;
}

.buydes-dialog-container-bottom-item {
  padding: 24rpx;
  display: flex;
  justify-content: center;
  border-bottom: 1rpx solid #eee;
  position: relative;
  overflow-y: scroll;
}

.buydes-dialog-container .dialog-content {
  padding: 20rpx;
  font-size: 24rpx;
  overflow-y: scroll;
  max-height: 430rpx;
}

.buydes-dialog-container .btn-confirm {
  background-color: #dfaf7d;
  height: 100rpx;
  width: 100%;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
}

.container-column .coupon {
  width: 710rpx;
  height: 212rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  border: none;
  font-size: 24rpx;
  color: #999;
}

.coupon .coupon-item-left {
  padding: 30rpx;
  width: 440rpx;
  height: 140rpx;
  background: #fff;
  margin-left: 6rpx;
  border-radius: 10rpx;
  box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.5);
}

.coupon .coupon-item-right {
  width: 200rpx;
  height: 200rpx;
  background: #dfaf7d;
  border-radius: 10rpx;
  text-align: center;
  line-height: 200rpx;
  color: #fff;
}

.godle {
  color: #dfaf7d;
  margin-top: 30rpx;
}

.buydes-dialog-container .rule-title {
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
  padding-top: 20rpx;
}

.buydes-dialog-container .rule-text {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
}

.buydes-dialog-container .modal-content {
  padding: 0 40rpx 40rpx 40rpx;
}

.countDownTimeView .countDownAllView .voteText {
  text-align: center;
  line-height: 30rpx;
  float: left;
  min-width: 30rpx;
  height: 30rpx;
  color: #fff;
  background: #fff !important;
}

/*促销  */

.promitionInfo {
  margin-bottom: 12rpx;
  background-color: #fff;
}

.promition, .coupon {
  margin-left: 40rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: rgb(51, 51, 51);
  font-family: PingFangSC-Regular;
}

.couponContent {
  width: 85%;
}

.coupon image, .promition image {
  width: 16rpx;
  height: 28rpx;
}

.coupon {
  border-bottom: solid rgb(241, 241, 241) 2rpx;
}

.promotionSign, .couponSign {
  width: 40rpx;
  height: 40rpx;
  font-size: 24rpx;
  background: rgb(204, 29, 37);
  color: #fff;
  margin-right: 16rpx;
  line-height: 40rpx;
  text-align: center;
}

.couponSign {
  background: rgb(223, 175, 125);
}

.recommend {
  padding-bottom: 500rpx;
  margin-bottom: 500rpx;
}

.recommend .recommend-item {
  float: left;
  margin-left: 30rpx;
  margin-bottom: 30rpx;
}

.recommend image {
  width: 330rpx;
  height: 330rpx;
  background: #eee;
}

.recommend .goods-name .price-now {
  font-size: 30rpx;
  color: rgb(51, 51, 51);
  padding: 0 20rpx;
}

.recommend .price-pre {
  font-size: 24rpx;
  color: rgb(153, 153, 153);
  position: relative;
  margin-left: 20rpx;
}

.recommend .price-pre::after {
  content: '';
  width: 100%;
  height: 2rpx;
  position: absolute;
  left: 0;
  top: 50%;
  background: rgb(153, 153, 153);
}

.toApp {
  width: 100%;
  height: 100rpx;
  background: #439df7;
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
  color: white;
  position: fixed;
  bottom: 0;
}

.group-booking {
  width: 100%;
  height: 86rpx;
  padding: 0 40rpx;
  overflow: hidden;
  color: #333;
  font-size: 24rpx;
  background: #fafafa;
  padding-bottom: 10rpx;
  position: relative;
  line-height: 86rpx;
  border-bottom: 2rpx solid rgb(241, 241, 241);
}

.count-down-container .final-payment {
  font-size: 24rpx;
  color: #fff;
  background: #e0bd93;
  padding: 4rpx 10rpx;
  margin-left: 100rpx;
  position: relative;
  top: -4rpx;
}

.fail-back {
  background: #999;
}

.market-promotion .limite-price {
  width: 78rpx;
  height: 30rpx;
  color: rgb(223, 175, 125);
  border: 2rpx solid rgb(223, 175, 125);
  border-radius: 4rpx;
  position: absolute;
  left: 180rpx;
  top: 30%;
  font-size: 22rpx;
  text-align: center;
  line-height: 30rpx;
  font-family: PingFangSC-Regular;
}
.btn-more{
  margin-top:20rpx;
}

.group-price {
  font-size: 28rpx;
  color: #333;
  font-weight: normal;
  margin-right: 20rpx;
}

.prograss-bar-container {
  margin-left: 30rpx;
}

.prograss-bar-container .prograss-bar-banner {
  float: left;
  width: 380rpx;
  height: 8rpx;
  background: #e0e0e0;
  margin-top: 16rpx;
  margin-right: 20rpx;
  border-radius: 5rpx;
}

.prograss-bar-banner .prograss-bar-item {
  max-width: 100%;
  height: 100%;
  background: -webkit-linear-gradient(left, #e7ceb6, #deaf80); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #e7ceb6, #deaf80); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #e7ceb6, #deaf80); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #e7ceb6, #deaf80); /* 标准的语法 */
  border-radius: 5rpx;
}

.earnest-double .double-tag {
  display: inline-block;
  float: left;
  background: #ff454a;
  padding: 2rpx 10rpx;
  font-size: 24rpx;
  color: #fff;
  line-height: 36rpx;
  border-radius: 4rpx;
  margin: 10rpx 30rpx 30rpx 30rpx;
}

.count-down-double {
  display: inline-block;
  position: relative;
  top: 10rpx;
  margin: 0 10rpx;
}

.modules .btn-more {
  width: 100%;
  text-align: center;
  height: 100rpx;
  font-size: 24rpx;
  color: #999;
}

.module-box {
  height: 250rpx;
  overflow: hidden;
}

.arrow .dot-top {
  font-size: 0;
  line-height: 0;
  border-width: 10px;
  border-color: red;
  border-top-width: 0;
  border-style: dashed;
  border-bottom-style: solid;
  border-left-color: transparent;
  border-right-color: transparent;
}

.btn-arrow-down {
  position: relative;
  width: 100%;
}

.btn-arrow-up {
  position: relative;
  width: 100%;
}

.btn-arrow-down::after {
  border: 10rpx solid transparent;
  border-top: 10rpx solid #999;
  width: 0;
  height: 0;
  position: absolute;
  content: ' ';
  right: 40%;
  top: 15rpx;
}

.btn-arrow-up::after {
  border: 10rpx solid transparent;
  border-bottom: 10rpx solid #999;
  width: 0;
  height: 0;
  position: absolute;
  content: ' ';
  right: 43%;
  top: 0;
}
